{
    "builds": [{
        "src": "bulk_email_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "100mb", "runtime": "python3.11" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bulk_email_project/wsgi.py"
        }
    ]
}
